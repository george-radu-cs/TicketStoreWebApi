{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "Server=<server-ip>,<server-port>;Database=<database-name> app;User Id=<user-name>;Password=<user-password>;MultipleActiveResultSets=true;"
  },
  "Jwt": {
    "SecretKey": "<your-jwt-secret-key>"
  }
}
